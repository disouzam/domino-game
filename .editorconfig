# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# .NET formatting rules
[*.{cs,vb}]

dotnet_diagnostic.IDE0055.severity = warning
indent_style = space
indent_size = 2
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false